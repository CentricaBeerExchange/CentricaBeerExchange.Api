{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "Issuer": "http://localhost",
    "Audience": "http://localhost",
    "Key": "<SomeKeyFromUserSecrets>",
    "ExpiryTime": "01:00:00"
  },
  "AuthSecrets": {
    "ReadOnly": {
      "Id": "ReadOnlyUser",
      "Secret": "<SomeValueFromUserSecrets>"
    },
    "ReadWrite": {
      "Id": "ReadWriteUser",
      "Secret": "<SomeValueFromUserSecrets>"
    }
  }
}
